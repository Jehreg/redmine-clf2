  </div>
  <!-- Message space ends / Fin de l'espace de message -->

  <nav role="navigation">
    <ul id="cn-ef-lang-links">
      <li><a href="/english" title="English - English version of the Web site">English</a></li>
      <li lang="fr"><a href="/francais" title="Français - Version française de ce site Web">Français</a></li>
    </ul>
  <div id="cn-wmms"><%= image_tag 'wmms.gif', :plugin => 'redmine_clf2', :width => "83", :height=>"20", :alt=>"Symbol of the Government of Canada / Symbole du gouvernement du Canada", :title => "Symbol of the Government of Canada / Symbole du gouvernement du Canada" %></div>
  </nav>
<!-- clf2-nsi2 theme ends / Fin du thème clf2-nsi2 -->
